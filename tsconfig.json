{
  "compilerOptions": {
    "module": "ES2015",
    "target": "es6",
    "outDir": "./dist/",
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "sourceMap": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
